= form_for(@image) do |f|
  - if @image.errors.any?
    #error_explanation
      %h2
        = pluralize(@image.errors.count, "error")
        prohibited this image from being saved:
      %ul
        - @image.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :url
    %br/
    = f.text_field :url
  .field
    = f.label :page_id
    %br/
    = f.text_field :page_id
  .actions
    = f.submit
